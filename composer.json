{
    "name": "issei-m/spike-php",
    "description": "The client of https://spike.cc REST api for PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Issei Murasawa",
            "email": "issei.m7@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Issei\\Spike\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Issei\\Spike\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
