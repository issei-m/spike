{
    "name": "issei-m/spike-php",
    "description": "The client of https://spike.cc REST api for PHP.",
    "keywords": ["spike", "spike.cc", "payments", "rest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Issei Murasawa",
            "email": "issei.m7@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.5"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~6.2",
        "phpunit/phpunit": "~4.8 || ~6.4",
        "symfony/var-dumper": "2.8.*"
    },
    "autoload": {
        "psr-4": {
            "Issei\\Spike\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Issei\\Spike\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle ~6.2 HTTP client"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.5.0"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
